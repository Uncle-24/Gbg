<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

    <system.diagnostics>
        <trace autoflush="true" indentsize="4">
          <listeners>
            <add name="serviceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Service.log"/>
            <remove name="Default"/>
          </listeners>
        </trace>
        <switches>
          <!--This variable represents the log level。0: no logging；1: only error；2: only error, waring；3: only error, waring, info；> = 4: error, waring, info, verbose-->
          <add name="LogSwitch" value="3"/>
        </switches>
      </system.diagnostics>
</configuration>